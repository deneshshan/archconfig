
ControlMaster auto
ControlPath ~/.ssh/ssh_control_%C
ControlPersist 60
ServerAliveInterval 15
ServerAliveCountMax 3

Host staging-app-2
  User deploy
  HostName ip-10-0-41-150.ec2.internal
  ForwardAgent yes
  ProxyCommand ssh -A denesh@bastion-legacy.staging.insight-out.com exec nc %h %p 2> /dev/null

